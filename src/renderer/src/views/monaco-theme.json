{
  "base": "vs-dark",
  "inherit": true,
  "rules": [
    { "token": "", "foreground": "dbdbdb", "background": "242629" },
    { "token": "comment", "foreground": "7a7a7a", "fontStyle": "italic" },
    { "token": "string", "foreground": "f14668" },
    { "token": "keyword", "foreground": "485fc7", "fontStyle": "bold" },
    { "token": "number", "foreground": "ffdd57" },
    { "token": "regexp", "foreground": "ff3860" },
    { "token": "operator", "foreground": "48c78e" },
    { "token": "namespace", "foreground": "3298dc" },
    { "token": "type", "foreground": "ffdd57" },
    { "token": "function", "foreground": "00d1b2" },
    { "token": "variable", "foreground": "dbdbdb" },
    { "token": "parameter", "foreground": "ffdd57", "fontStyle": "italic" },
    { "token": "property", "foreground": "00d1b2" },
    { "token": "builtin", "foreground": "ff3860" },
    { "token": "punctuation", "foreground": "dbdbdb" },
    { "token": "tag", "foreground": "f14668" },
    { "token": "attribute.name", "foreground": "ffdd57" },
    { "token": "attribute.value", "foreground": "00d1b2" },
    { "token": "meta", "foreground": "7a7a7a" },
    { "token": "invalid", "foreground": "ffffff", "background": "f14668" }
  ],
  "colors": {
    "editor.foreground": "#dbdbdb",
    "editor.background": "#14161a",
    "editor.selectionBackground": "#485fc740",
    "editor.inactiveSelectionBackground": "#485fc720",
    "editor.lineHighlightBackground": "#363636",
    "editorCursor.foreground": "#ffdd57",
    "editorWhitespace.foreground": "#4a4a4a",
    "editorIndentGuide.background": "#4a4a4a",
    "editorIndentGuide.activeBackground": "#7a7a7a",
    "editor.selectionHighlightBackground": "#ffdd5720",
    "editor.findMatchBackground": "#00d1b250",
    "editor.findMatchBorder": "#00d1b2",
    "editor.hoverHighlightBackground": "#ffdd5720",
    "editor.wordHighlightBackground": "#ffdd5720",
    "editor.lineNumber.foreground": "#7a7a7a",
    "editor.lineNumber.activeForeground": "#dbdbdb",
    "editorBracketMatch.background": "#00d1b220",
    "editorBracketMatch.border": "#00d1b2"
  }
}
